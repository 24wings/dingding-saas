    <div *ngIf="showTenantChange()">
      <app-tenant-change></app-tenant-change>
    </div>
    @Body
    <global-footer [links]="links">
      Copyright
      <i nz-icon nzType="copyright"></i>
      2020
      <a href="//shop.fotaboss.com" target="_blank">深移联科技</a>
      出品
    </global-footer>
  </div>
