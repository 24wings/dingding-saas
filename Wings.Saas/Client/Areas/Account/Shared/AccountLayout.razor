

@inherits LayoutComponentBase


<div class="container">
  <pro-langs class="langs" btnClass></pro-langs>
  <div class="wrap">
    <div class="top">
      <div class="head">
        <img class="logo" src="./assets/logo-color.svg">
        <span class="title">电商供应链系统</span>
      </div>
      <div class="desc">高效、便捷的电商供应链解决方案</div>
